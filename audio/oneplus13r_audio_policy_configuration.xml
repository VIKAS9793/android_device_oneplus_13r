<?xml version="1.0" encoding="utf-8"?>
<audio-policy-configuration>
    <device-descriptors>
        <device-descriptor>
            <name>Speaker</name>
            <type>speaker</type>
            <id>0</id>
            <address>0</address>
            <profile>
                <sampling-rate>48000</sampling-rate>
                <channel-count>2</channel-count>
                <format>pcm_16</format>
            </profile>
        </device-descriptor>
        <device-descriptor>
            <name>Headset</name>
            <type>headset</type>
            <id>1</id>
            <address>0</address>
            <profile>
                <sampling-rate>48000</sampling-rate>
                <channel-count>2</channel-count>
                <format>pcm_16</format>
            </profile>
        </device-descriptor>
        <device-descriptor>
            <name>Bluetooth</name>
            <type>bluetooth</type>
            <id>2</id>
            <address>0</address>
            <profile>
                <sampling-rate>48000</sampling-rate>
                <channel-count>2</channel-count>
                <format>pcm_16</format>
            </profile>
        </device-descriptor>
        <device-descriptor>
            <name>USB</name>
            <type>usb</type>
            <id>3</id>
            <address>0</address>
            <profile>
                <sampling-rate>48000</sampling-rate>
                <channel-count>2</channel-count>
                <format>pcm_16</format>
            </profile>
        </device-descriptor>
    </device-descriptors>
    <audio-attributes>
        <attribute>
            <name>Media</name>
            <type>media</type>
            <usage>media</usage>
            <content-type>music</content-type>
            <flags>none</flags>
        </attribute>
        <attribute>
            <name>Communication</name>
            <type>communication</type>
            <usage>communication</usage>
            <content-type>speech</content-type>
            <flags>none</flags>
        </attribute>
        <attribute>
            <name>System</name>
            <type>system</type>
            <usage>system</usage>
            <content-type>sonification</content-type>
            <flags>none</flags>
        </attribute>
        <attribute>
            <name>Alarm</name>
            <type>alarm</type>
            <usage>alarm</usage>
            <content-type>alarm</content-type>
            <flags>none</flags>
        </attribute>
        <attribute>
            <name>Notification</name>
            <type>notification</type>
            <usage>notification</usage>
            <content-type>sonification</content-type>
            <flags>none</flags>
        </attribute>
    </audio-attributes>
    <audio-profiles>
        <profile>
            <name>Default</name>
            <sampling-rate>48000</sampling-rate>
            <channel-count>2</channel-count>
            <format>pcm_16</format>
        </profile>
        <profile>
            <name>HighQuality</name>
            <sampling-rate>96000</sampling-rate>
            <channel-count>2</channel-count>
            <format>pcm_24</format>
        </profile>
        <profile>
            <name>LowLatency</name>
            <sampling-rate>48000</sampling-rate>
            <channel-count>2</channel-count>
            <format>pcm_16</format>
        </profile>
    </audio-profiles>
    <audio-modes>
        <mode>
            <name>Normal</name>
            <type>normal</type>
            <profile>Default</profile>
        </mode>
        <mode>
            <name>LowLatency</name>
            <type>low_latency</type>
            <profile>LowLatency</profile>
        </mode>
        <mode>
            <name>HighQuality</name>
            <type>high_quality</type>
            <profile>HighQuality</profile>
        </mode>
    </audio-modes>
    <audio-effects>
        <effect>
            <name>Equalizer</name>
            <type>equalizer</type>
            <profile>Default</profile>
        </effect>
        <effect>
            <name>BassBoost</name>
            <type>bass_boost</type>
            <profile>Default</profile>
        </effect>
        <effect>
            <name>Virtualizer</name>
            <type>virtualizer</type>
            <profile>Default</profile>
        </effect>
        <effect>
            <name>Reverb</name>
            <type>reverb</type>
            <profile>Default</profile>
        </effect>
    </audio-effects>
    <audio-routes>
        <route>
            <name>Speaker</name>
            <source>Speaker</source>
            <sink>Speaker</sink>
            <profile>Default</profile>
        </route>
        <route>
            <name>Headset</name>
            <source>Headset</source>
            <sink>Headset</sink>
            <profile>Default</profile>
        </route>
        <route>
            <name>Bluetooth</name>
            <source>Bluetooth</source>
            <sink>Bluetooth</sink>
            <profile>Default</profile>
        </route>
        <route>
            <name>USB</name>
            <source>USB</source>
            <sink>USB</sink>
            <profile>Default</profile>
        </route>
    </audio-routes>
</audio-policy-configuration>
