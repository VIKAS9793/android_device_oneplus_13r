<?xml version="1.0" encoding="utf-8"?>
<media-profiles>
    <video-encoders>
        <encoder>
            <name>H264</name>
            <mime>video/avc</mime>
            <profile>high</profile>
            <level>5.1</level>
            <max-width>3840</max-width>
            <max-height>2160</max-height>
            <max-bitrate>50000000</max-bitrate>
            <max-fps>60</max-fps>
        </encoder>
        <encoder>
            <name>HEVC</name>
            <mime>video/hevc</mime>
            <profile>main</profile>
            <level>5.1</level>
            <max-width>3840</max-width>
            <max-height>2160</max-height>
            <max-bitrate>50000000</max-bitrate>
            <max-fps>60</max-fps>
        </encoder>
    </video-encoders>
    <video-decoders>
        <decoder>
            <name>H264</name>
            <mime>video/avc</mime>
            <profile>high</profile>
            <level>5.1</level>
            <max-width>3840</max-width>
            <max-height>2160</max-height>
            <max-fps>60</max-fps>
        </decoder>
        <decoder>
            <name>HEVC</name>
            <mime>video/hevc</mime>
            <profile>main</profile>
            <level>5.1</level>
            <max-width>3840</max-width>
            <max-height>2160</max-height>
            <max-fps>60</max-fps>
        </decoder>
    </video-decoders>
    <audio-encoders>
        <encoder>
            <name>AAC</name>
            <mime>audio/mp4a-latm</mime>
            <profile>lc</profile>
            <max-bitrate>320000</max-bitrate>
            <max-channels>6</max-channels>
            <max-sampling-rate>48000</max-sampling-rate>
        </encoder>
    </audio-encoders>
    <audio-decoders>
        <decoder>
            <name>AAC</name>
            <mime>audio/mp4a-latm</mime>
            <profile>lc</profile>
            <max-channels>6</max-channels>
            <max-sampling-rate>48000</max-sampling-rate>
        </decoder>
        <decoder>
            <name>MP3</name>
            <mime>audio/mpeg</mime>
            <max-channels>2</max-channels>
            <max-sampling-rate>48000</max-sampling-rate>
        </decoder>
    </audio-decoders>
</media-profiles>
